{
    "name": "laraedit/laraedit",
    "description": "A web-based IDE package for Laravel 5.",
    "keywords": ["laravel", "ide", "editor", "artisan"],
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Bourgeois",
            "email": "derek@ibourgeois.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0"
    },
    "autoload": {
        "classmap": [
            "src/Controllers"
        ],
        "psr-4": {
            "LaraEdit\\LaraEdit\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": "php artisan vendor:publish",
        "post-update-cmd": "php artisan vendor:publish --tag=public --force"
    }
}
